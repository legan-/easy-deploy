{
  "name": "@legan/deploy",
  "description": "Deploy easily",
  "author": "legan",
  "version": "1.1.0",
  "main": "./lib/bin.js",
  "license": "ISC",
  "bin": {
    "easy-deploy": "./lib/bin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "node-ssh": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-root-import": "^6.1.0",
    "eslint": "^5.7.0",
    "eslint-watch": "^4.0.2",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "files": [
    "LICENSE",
    "lib/bin.js",
    "lib/deploy.js"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "babel-plugin-root-import"
    ]
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "esw ./src/*.js --color",
    "build": "babel src --out-dir lib",
    "prepublish": "npm-run-all clean lint build"
  }
}
